<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE en-note SYSTEM 'http://xml.evernote.com/pub/enml2.dtd'>
<en-note>
  <h1># Twitter</h1>
  <ul>
    <% @twitter.each do |t| %>
      <li><%= t %></li>
    <% end %>
  </ul>

  <h1># Moves</h1>
  <h2>## Summary</h2>
  <ul>
    <li>Walk: <%= @moves['summary']['walk'] %>km</li>
    <li>Cycle: <%= @moves['summary']['cycle'] %>km</li>
    <li>Transport: <%= @moves['summary']['transport'] %>km</li>
    <li><%= @moves['summary']['cal'] %>cal</li>
  </ul>
  <h2>## Detail</h2>
  <% @moves['activity'].each do |act| %>
    <h3><%= act['place'] %></h3>
    <% unless act['move'].nil? %>
      <% act['move'].each do |m| %>
        <ul>
          <li><%= m['action'] %></li>
          <li><%= m['time'] %></li>
          <li><%= m['distance'] %></li>
        </ul>
      <% end %>
    <% end %>
  <% end %>
</en-note>
